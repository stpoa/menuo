{
  "name": "@menuo/server",
  "version": "1.0.0",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "start": "yarn start:server",
    "start:server": "node ./scripts/start-local.js",
    "start:client": "cd ../client && yarn start",
    "deploy:staging": "sls deploy --stage dev --env staging",
    "deploy:production": "sls deploy --stage prod --env production",
    "format": "npm run format:prettier",
    "format:prettier": "prettier 'src/**/*.ts*' --write",
    "test": "run-p test:types test:logic",
    "test:logic": "jest",
    "test:types": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@menuo/shared": "link:../shared",
    "aws-sdk": "^2.601.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "logdna": "^3.4.0",
    "mongodb": "^3.4.1",
    "mongodb-client-encryption": "^1.0.1",
    "ramda": "^0.26.1",
    "serverless-finch": "^2.4.3",
    "source-map-support": "^0.5.10",
    "web-push": "^3.4.3",
    "yaml-tag": "^1.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^24.0.22",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lambda-log": "^2.2.0",
    "@types/mongodb": "^3.3.14",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.26.36",
    "@types/web-push": "^3.3.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^24.9.0",
    "localtunnel": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "serverless": "^1.57.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "5.12.1",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "3.8.1-rc",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
