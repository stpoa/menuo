{
  "name": "@menuo/waiter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "test": "react-scripts test",
    "qr:staging": "env-cmd -f .env.staging scripts/qr-image.js",
    "qr:production": "env-cmd -f .env.production scripts/qr-image.js",
    "eject": "react-scripts eject",
    "deploy:staging": "yarn build:staging && yarn upload:staging",
    "deploy:production": "yarn build:production && yarn upload:production",
    "upload:staging": "aws s3 sync ./build s3://menuo.me/waiter",
    "upload:production": "aws s3 sync ./build s3://menuo.app/waiter",
    "upload:stageless": "cd ../server && sls client deploy --no-config-change --no-policy-change --no-cors-change --no-delete-contents --no-confirm"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@menuo/shared": "link:../shared",
    "@reduxjs/toolkit": "^1.2.3",
    "@types/ramda": "^0.26.39",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "axios": "^0.19.0",
    "axios-hooks": "^1.7.2",
    "connected-react-router": "^6.7.0",
    "history": "^4.10.1",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-gtm-module": "^2.0.8",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.4",
    "typesafe-actions": "^5.1.0",
    "typescript": "3.8.1-rc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/ip": "^1.1.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-router-hash-link": "^1.2.1",
    "env-cmd": "^10.0.1",
    "ip": "^1.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:3000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
