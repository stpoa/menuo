{
  "name": "menuo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "run-p start:server start:client",
    "deploy": "run-p deploy:server deploy:client",
    "start:client": "cd ./client && yarn start",
    "deploy:client": "run-s build:client upload:client",
    "build:client": "cd ./client && yarn build",
    "upload:client": "cd ./server && sls client deploy --no-config-change --no-policy-change --no-cors-change --no-confirm",
    "start:server": "cd ./server && yarn start",
    "deploy:server": "cd ./server && yarn deploy",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
