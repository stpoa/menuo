{
  "name": "menuo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "run-p start:server start:client start:shared",
    "deploy": "run-p deploy:server deploy:client",
    "start:client": "cd ./packages/client && yarn start",
    "deploy:client": "cd ./packages/client && yarn deploy",
    "upload:client": "cd ./packages/server && sls client deploy --no-config-change --no-policy-change --no-cors-change --no-confirm",
    "start:server": "cd ./packages/server && yarn start",
    "deploy:server:dev": "cd ./packages/server && yarn deploy:dev",
    "deploy:server:prod": "cd ./packages/server && yarn deploy:prod",
    "start:shared": "cd ./packages/shared && yarn start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {}
}
